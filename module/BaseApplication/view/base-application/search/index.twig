{% extends "layout/layout.twig" %}

{% block page_title %}{{ result.title }} - {% endblock %}

{% block content %}
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">{{ result.title }}</h1>
        </div>
    </div>

    <div class="row">
        <div class="col-lg-12">
            {{ result.data|length }} {{ translate('results found') }}
            {% include 'partials/flash-messages.twig' %}
        </div>
    </div>
{% endblock %}
